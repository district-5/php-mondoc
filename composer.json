{
    "name": "district5/mondoc",
    "description": "Mondoc is a lightweight and easy to use model and service based abstraction library for MongoDB",
    "keywords": ["mondoc", "mongo", "mongodb", "odm", "library", "district5"],
    "homepage": "https://github.com/district-5/php-mondoc",
    "license": "mit",
    "authors": [
        {
            "name": "District5",
            "email": "hello@district5.co.uk"
        }
    ],
    "repositories": {
        "district5/mondoc-encryption": {
            "type": "vcs",
            "url": "git@github.com:district-5/php-mondoc-encryption.git"
        }
    },
    "require": {
        "php": ">=8.2.0",
        "ext-mongodb": "*",
        "mongodb/mongodb": "*",
        "district5/date": ">=3.0.4",
        "district5/mondoc-builder": "*"
    },
    "require-dev": {
        "district5/mondoc-encryption": ">=0.0.4",
        "phpunit/phpunit": "*"
    },
    "support": {
        "issues": "https://github.com/district-5/php-mondoc/issues",
        "source": "https://github.com/district-5/php-mondoc",
        "docs": "https://mondoc.district5.dev"
    },
    "suggest": {
        "ext-sodium": "If leveraging field encryption with the Mondoc Encryption library."
    },
    "autoload": {
        "psr-4": {
            "District5\\Mondoc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "District5Tests\\MondocTests\\": "tests/MondocTests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
